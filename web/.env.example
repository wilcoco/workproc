# Server
NODE_ENV=development
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=

# Database (Railway PostgreSQL)
DATABASE_URL=postgresql://USER:PASSWORD@HOST:PORT/DB?schema=public

# OAuth (Auth.js / NextAuth)
AUTH_GITHUB_ID=
AUTH_GITHUB_SECRET=
AUTH_GOOGLE_ID=
AUTH_GOOGLE_SECRET=

# Credentials Auth Controls
# true로 설정하면 /api/auth/signup 이 비활성화됩니다(운영 권장)
DISABLE_SIGNUP=false

# Seed Admin (web/prisma/seed.mjs에서 사용)
ADMIN_EMAIL=
ADMIN_PASSWORD=
ADMIN_USERNAME=
ADMIN_NAME=
